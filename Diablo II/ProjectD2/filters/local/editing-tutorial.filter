// Other than gambling items and runewords, all halting rules in this category are for non-class items (class items have automods/pointmods to account for which can take a lot of rules)

// Gambling Items
ItemDisplay[NMAG !ID]: %NAME%

// Runewords
ItemDisplay[RW]: %NAME%{%NAME%}%WHITE%

// Name Modification: Superior/Sockets
ItemDisplay[NMAG SUP ED>0]: %NAME% %BLUE%+%ED%{%NAME%}%CONTINUE%
ItemDisplay[NMAG SOCK>0]: %NAME% %GRAY%[%SOCKETS%]{%NAME%}%CONTINUE%

// Descriptions - Price
ItemDisplay[NMAG !EQUIPPED !SHOP !GEMMED !aqv !cqv]:	%NAME%{%WHITE%Sell Value: %PRICE%}%CONTINUE%	// adds price

// Descriptions for Socketing Recipes (for 3+ socket-capable items above ilvl 25)
ItemDisplay[FILTLVL=0 NMAG SOCK=0 !INF !SUP ILVL>25 !EQUIPPED !SHOP (HELM OR CIRC) (DRU OR BAR OR (ILVL>40 (ci2 OR ci3 OR msk OR xsk OR usk OR ghm OR xhm OR uhm OR crn OR xrn OR urn OR uh9)))]:		%NAME%{%NAME%%NL%%WHITE%Socketing Recipe: %ORANGE%[%BLUE%O%ORANGE%]%GRAY%+%TAN%(%WHITE%8%TAN%)%GRAY%+%TAN%(%WHITE%10%TAN%)}%CONTINUE%							// socketing recipes for helms   (ilvl 25+ 3+ socket-capable)
ItemDisplay[FILTLVL=0 NMAG SOCK=0 !INF !SUP ILVL>25 !EQUIPPED !SHOP CHEST (ELT OR rng OR xng OR brs OR xrs OR gth OR xth OR ltp OR xtp OR ful OR xul OR aar OR xar OR xcl OR xhn OR xpl OR xlt OR xld)]:	%NAME%{%NAME%%NL%%WHITE%Socketing Recipe: %ORANGE%[%YELLOW%O%ORANGE%]%GRAY%+%TAN%(%WHITE%7%TAN%)%GRAY%+%TAN%(%WHITE%10%TAN%)}%CONTINUE%							// socketing recipes for chests  (ilvl 25+ 3+ socket-capable)
ItemDisplay[FILTLVL=0 NMAG SOCK=0 !INF !SUP ILVL>25 !EQUIPPED !SHOP SHIELD (DIN OR (NEC !NORM) OR lrg OR xrg OR urg OR kit OR xit OR uit OR tow OR xow OR uow OR gts OR xts OR uts OR upk OR ush)]:	%NAME%{%NAME%%NL%%WHITE%Socketing Recipe: %ORANGE%[%RED%O%ORANGE%]%GRAY%+%TAN%(%WHITE%7%TAN%)%GRAY%+%TAN%(%WHITE%11%TAN%)}%CONTINUE%							// socketing recipes for shields (ilvl 25+ 3+ socket-capable)
