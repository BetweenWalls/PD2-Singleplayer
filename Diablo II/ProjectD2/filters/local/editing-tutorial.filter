ItemDisplay[lpp]:	%MAP-53%%TIER-9%    %NAME%    {%WHITE%Cannot be used with unique or set items%NL%Adds 2-4 sockets to two-handed weapons, or 1-2 sockets to other items}%WHITE%		// Larzuk's Puzzlepiece
ItemDisplay[lbox]:	%MAP-53%%TIER-9%        %NAME%        {%WHITE%Adds 2-4 sockets to two-handed weapons, or 1-2 sockets to other items}%WHITE%	// Larzuk's Puzzlebox
ItemDisplay[rtp]:	%BORDER-53%%TIER-9%     %NAME%     {%NAME%}%WHITE%										// Horadrim Navigator
